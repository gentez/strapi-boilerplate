      "components": [
        "pages-component.column-1",
        "pages-component.column-2",
        "pages-component.column-3"
      ]
    },
    "Navbar": {
      "type": "component",
      "repeatable": false,
      "component": "pages-component.navbar"
    },
    "layout_test": {
      "type": "component",
      "repeatable": false,
      "component": "pages-component.column"
    },
    "footer": {
      "type": "component",
      "repeatable": false,
      "component": "pages-component.footer"
    }
